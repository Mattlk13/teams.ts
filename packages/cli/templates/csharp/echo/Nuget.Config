<!-- Note: Only needed until we release to Nuget -->
<!-- https://docs.github.com/en/packages/working-with-a-github-packages-registry/working-with-the-nuget-registry -->
<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <packageSources>
        <add key="github" value="https://nuget.pkg.github.com/microsoft/index.json" />
    </packageSources>
    <packageSourceCredentials>
        <!-- GitHub Package Credentials -->
        <github>
            <add key="Username" value="USERNAME" /> <!-- Replace with your github username -->
            <add key="ClearTextPassword" value="TOKEN" /> <!-- Replace with your github token -->
        </github>
    </packageSourceCredentials>
</configuration>
